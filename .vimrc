call pathogen#infect()
filetype indent plugin on
set autoindent
set smartindent

set tabstop=4
set shiftwidth=4
set expandtab
set showmatch

set vb t_vb=

set nohls
set incsearch

set background=dark
syntax on
highlight Pmenu ctermfg=green ctermbg=8
highlight PmenuSel ctermfg=green ctermbg=7
let g:jedi#autocompletion_command = "<Tab>"
